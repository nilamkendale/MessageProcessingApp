package messageprocessing;

/**
 * @author Nilam
 * POJO of Product details with default and parameterize constructor
 *
 */
public class ProductDetails {

		private String item;
		private String value;
		private int numberOfSale;

		public int getNumberOfSale() {
			return numberOfSale;
		}
		public void setNumberOfSale(int numberOfSale) {
			this.numberOfSale = numberOfSale;
		}
		public String getItem() {
			return item;
		}
		public void setItem(String item) {
			this.item = item;
		}
		public String getValue() {
			return value;
		}
		public void setValue(String value) {
			this.value = value;
		}
		
		public ProductDetails() {
		}
		 
		public ProductDetails(String string) {
			item = string;
		}
		
		
}
